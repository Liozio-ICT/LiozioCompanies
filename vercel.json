{
    "builds": [
      {
        "src": "LiozioCompanies/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "builds.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "LiozioCompanies/wsgi.py"
      }
    ]
}